<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="agri_adjustment_operator_equal" model="agri.adjustment.operator">
            <field name="sequence">1</field>
            <field name="name">Equal To</field>
            <field name="symbol">==</field>
            <field name="operator_type">general</field>
        </record>
        
        <record id="agri_adjustment_operator_not_equal" model="agri.adjustment.operator">
            <field name="sequence">2</field>
            <field name="name">Not Equal To</field>
            <field name="symbol">!=</field>
            <field name="operator_type">general</field>
        </record>
        
        <record id="agri_adjustment_operator_between" model="agri.adjustment.operator">
            <field name="sequence">3</field>
            <field name="name">Between</field>
            <field name="symbol">in</field>
            <field name="operator_type">numeric</field>
        </record>
        
        <record id="agri_adjustment_operator_less_than" model="agri.adjustment.operator">
            <field name="sequence">4</field>
            <field name="name">Less Than</field>
            <field name="symbol">&lt;</field>
            <field name="operator_type">numeric</field>
        </record>
        
        <record id="agri_adjustment_operator_less_than_equal_to" model="agri.adjustment.operator">
            <field name="sequence">5</field>
            <field name="name">Less Than or Equal To</field>
            <field name="symbol">&lt;=</field>
            <field name="operator_type">numeric</field>
        </record>
        
        <record id="agri_adjustment_operator_greater_than_equal_to" model="agri.adjustment.operator">
            <field name="sequence">6</field>
            <field name="name">Greater Than or Equal To</field>
            <field name="symbol">&gt;=</field>
            <field name="operator_type">numeric</field>
        </record>
        
        <record id="agri_adjustment_operator_greater_than" model="agri.adjustment.operator">
            <field name="sequence">7</field>
            <field name="name">Greater Than</field>
            <field name="symbol">&gt;</field>
            <field name="operator_type">numeric</field>
        </record>
          
    </data>
</odoo>
